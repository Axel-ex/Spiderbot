use core::f32;

use crate::constants::*;

pub struct Gait {
    current_pos: [[f32; 3]; 4], // real time coordinates of the end of each leg
    expected_pos: [[f32; 3]; 4], // expected coordinates
    temp_speed: [[f32; 3]; 4],
    config: RobotConfig,
}

impl Gait {
    pub fn new() -> Self {
        let current_pos = [[0.0; 3]; 4];
        let config = RobotConfig::new();

        current_pos[0] = {X_DEFAULT, 23.0, 34.0};
    }
}

